{
  "rules": [
    {
      "description": "From contains hdfc alerts â†’ mark read & move",
      "match": "all",
      "conditions": [
        {"field": "From", "predicate": "contains", "value": "noreply@github.com"},
        {"field": "DateReceived", "predicate": "less_than_days", "value": 1}
      ],
      "actions": [
        {"type": "mark_as_read"},
        {"type": "move_message", "mailbox": "Notifications"}
      ]
    }
  ]
}
